.container{
    width: 100%;
    height: 100%;
  }
  .film{
    margin-left:30rpx;
    margin-top: 20rpx;
    width: 92%;
    font-weight: 550;
    font-size: 35rpx;
    border-bottom: 1rpx solid #d3d1d1;
    height: 70rpx;
    line-height:50rpx;
  }
  .star{
    width: 92%;
    display: flex;
    border-bottom: 1rpx solid #d3d1d1;
    height: 70rpx;
    margin-left:30rpx;
  }
  .star_name{
    align-self: center;
    font-size: 35rpx;
    font-weight: 500;
  }
  .star_star{
    flex: 1;
    text-align: right;
    align-self: center;
  }
  .wux-rater__text {
    margin-left: 30rpx;
    align-self: center;
    font-size: 35rpx;
    color: #ffc900;
    margin-right: 20rpx;
}
  .container .sign{
    color: red;
  }
  .container .input_view{
    width: 92%;
    margin-left:30rpx;
    height: 420rpx;
    margin-top:25rpx;
    border-bottom: 1rpx solid #d3d1d1;
  }
  .input_view .text{
    margin-left:30rpx ;
    font-size: 28rpx;
  }
  .input{
    width: 100%;
    height: 400rpx;
    margin-top: 20rpx;
    font-size: 28rpx;
  }
  .placeholder-style{
    font-size: 30rpx;
    margin-left: 20rpx;
    color: rgb(155, 150, 150);
  }
  .submit{
    display: flex;
    flex-direction: row;
  }
  .submit .button_change{
    width: 90%;
    background:linear-gradient(to right,#0385f5,#17b4db);
    border: none;
    color: white;
    padding: 20rpx 64rpx;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 28rpx;
    margin-top: 30rpx;
    border-radius:20rpx ;
    height: 80rpx;
  }
  